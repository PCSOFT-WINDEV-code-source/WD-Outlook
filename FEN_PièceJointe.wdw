  procedures :
   -
     name : ExtraitPieceJointe
     internal_properties : CAAAAAgAAACK5Zbgb1cAEeiYNyGGBHu+uUb+ClbRHcYanPQBkeoKG0wJi7oplDBJQmo22E746a7q0uaDdmg1k5JO3Qb8rtThGRooDKgo3y4AqJIDKxAfREVKuoy8sT5dO2iTcy6zA6qqWXoqA8RoIxfCfZ5HxxpH4EZcDfust8y3PKvnDBzMARHTiwmqBUwp1/R2ftO3YhjLpLe2aWI=
     procedure_id : 1407900578978239834
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] ExtraitPieceJointe (<sNomPJ> [, <sRepTemp>])
      //
      // Paramètres :
      //	sNomPJ : <Nom de la pièce jointe>
      // 	sRepTemp (valeur par défaut="") : < indiquez ici le rôle de sRepTemp >
      // Valeur de retour :
      // 	chaîne : 
      //
      Procédure ExtraitPieceJointe(sNomPJ,sRepTemp="")
      
      QUAND EXCEPTION DANS
      	SI  sRepTemp~="" ALORS sRepTemp=gsRepTempPJ
      	
      	// Re-positionnemment sur l'objet OutLook correspondant si on n'est pas en création d'un nouveau message mais en relecture d'un ancien
      	si email.ID <> "" alors
      		SI PAS	EmailLitMessage(gnIdSession,Email.ID) ALORS
      			Erreur(<§$0006§>,ErreurInfo(errMessage))
      			RENVOYER ""
      		FIN
      		
      	FIN
      	
      	
      	// Recherche de la pièce jointe demandée
      	sNomFichierAttaché est une chaine
      	sNomFichierCopié est une chaine 
      	nNbMsg est un entier = Email.NbAttache
      	
      	POUR i = 1 A nNbMsg
      		sNomFichierAttaché = email.Attache[i]
      		SI sNomFichierAttaché ~= sNomPJ ALORS
      			// Extraction dans le répertoire courant
      			sNomFichierCopié=sRepTemp + sNomFichierAttaché
      			EmailSauveFichierAttaché(sNomFichierAttaché,sNomFichierCopié) 
      			RENVOYER sNomFichierCopié
      		FIN
      	FIN
      	
      	RENVOYER ""
      	
      FAIRE
      	Erreur(ExceptionInfo(errRésumé))
      	RepriseSaisie()
      FIN
      
      RENVOYER ""
