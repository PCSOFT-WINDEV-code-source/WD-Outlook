     code : |1+
      procedure FEN_FicheMail(gsMode = CST_MODIFICATION,gsType = cst_repondre)
      
      gsDate est une chaine
      gsHeure est une chaine
      
      // Liste des destinataires possibles lue dans le dossier "Contacts" d'OutLook
      RemplitListeEmails()
      	
      si gsMode~=CST_MODIFICATION alors
      	// Affichage du message reçu
      	IDMESSAGE = email.ID
      	gsDate = Email.DateRéception[[1 a 8]]
      	gsHeure = Email.DateRéception[[9 a 12]]
      	SAI_Date=gsDate
      	SAI_HEURE=gsHeure
      	SAI_Expéditeur=email.Destinataire
      	SAI_Contenu=	rc+rc+<§$0000§>+rc+...
      				<§$0001§>+email.Expediteur+rc+...
      				<§$0002§>+	DateVersChaîne(gsDate, maskDateSystème)+<§$0012§>+heureverschaine(gsHeure,"HH:MM")+rc+...
      				<§$0003§>+email.Sujet+rc+rc+email.Message
      	selon email.NbAttache
      		cas 0 : BTN_PJ..libelle=<§$0004§>
      		cas 1 : BTN_PJ..libelle=<§$0005§>
      		autres cas : BTN_PJ..libelle=ChaîneConstruit(<§$0006§>, Email.NbAttache)
      	fin
      	selon Majuscule(SansEspace(gsType))
      		cas cst_repondre
      			lib_titre=<§$0007§>
      			SAI_Sujet=<§$0008§>+eMail.Sujet
      			SAI_Destinataire=email.Expediteur
      		cas cst_repondre_a_tous
      			lib_titre=<§$0009§>
      			SAI_Sujet=<§$0008§>+email.Sujet
      			SAI_Destinataire=email.Expediteur
      			si pas email.Destinataire~="" alors SAI_Destinataire+="; "+Email.Destinataire
      		cas cst_FAIRE_SUIVRE
      			LIB_Titre=<§$000a§>
      			SAI_Sujet=<§$000b§>+email.Sujet
      	fin
      	ecranpremier(SAI_Contenu)
      sinon
      	LIB_Titre=<§$0010§>
      	//Remise à zéro des constantes email
      	EmailRAZ()
      	// Valeurs par défaut
      	SAI_Expéditeur=gsExpediteur
      	SAI_Date..libelle=<§$000c§>
      	SAI_Date=datesys() ; 
      	SAI_HEURE=heuresys()
      	
      	BTN_PJ..libelle=<§$0004§>
      FIN
      
