  procedures :
   -
     name : RemplitListeEmails
     procedure_id : 1407900347048509637
     type_code : 14
     code : |1+
      Procédure RemplitListeEmails()
      
      // Liste des destinataires possibles lue dans le dossier "Contacts" d'OutLook
      
      listeSupprimeTout(SAI_Destinataire)
      listeSupprimeTout(SAI_DestCopie)
      listeSupprimeTout(SAI_DestCopieC)
      contactPremier(gnIdSession)
      tantque pas mContact.EnDehors 
      	si mContact.Email <> "" alors
      		listeAjoute(SAI_Destinataire,mContact.EMail)
      		listeAjoute(SAI_DestCopie,mContact.Email)
      		listeAjoute(SAI_DestCopieC,mContact.Email)
      	fin
      	contactSuivant(gnIdSession)
      FIN
      
      
