      -
        code : |1-
         sListeDest est une chaine
         sListeCC est une chaine
         sListeCCi est une chaine
         sDest est une chaine
         sCC est une chaine
         sCCi est une chaine
         
         // Contrôles de saisie
         SI SAI_Sujet~="" ALORS
         	Erreur(<§$0027§>)
         	RepriseSaisie(SAI_Sujet)
         FIN
         SI SAI_Contenu~="" ALORS
         	Erreur(<§$0028§>)
         	RepriseSaisie(SAI_Contenu)
         FIN
         SI SAI_Destinataire~="" ALORS
         	Erreur(<§$0029§>)
         	RepriseSaisie(SAI_Destinataire)
         FIN
         
         // Identifiant
         Email.MessageID = IDMESSAGE
         // Sujet
         Email.Sujet = SAI_Sujet
         // Contenu de la note
         Email.Message = SAI_Contenu
         // Dates
         Email.DateRéception = DateVersChaîne(SAI_Date, maskDateSystème)+" "+ Gauche(HeureVersChaîne(SAI_HEURE),5)
         // Expéditeur
         Email.Expéditeur = SAI_Expéditeur
         
         sListeDest = Remplace(SAI_Destinataire," ",";")
         sListeDest = Remplace(SAI_Destinataire,",",";")
         POUR TOUTE CHAÎNE sDest DE sListeDest SEPAREE PAR ";"
         	Email.NbDestinataire++
         	Email.Destinataire[Email.NbDestinataire] = sDest	
         FIN
         
         sListeCC = Remplace(SAI_DestCopie," ",";")
         sListeCC = Remplace(SAI_DestCopie,",",";")
         POUR TOUTE CHAÎNE sCC DE sListeCC SEPAREE PAR ";"
         	Email.NbCc++
         	Email.Cc[Email.NbCc] =  sCC
         FIN
         
         sListeCCi = Remplace(SAI_DestCopieC," ",";")
         sListeCCi = Remplace(SAI_DestCopieC,",",";")
         POUR TOUTE CHAÎNE sCCi DE sListeCCi SEPAREE PAR ";"
         	Email.NbCci++
         	Email.Cci[Email.NbCci] =  sCCi
         FIN
         
         
         // Envoi dans OutLook
         //Pour info : l'utilisation d'un nom du carnet d'adresses comme destinataire est possible
         SI PAS EmailEnvoieMessage(gnIdSession) ALORS
         	Erreur (<§$002a§>, ErreurInfo(errMessage))
         FIN
         
         
         //Exécute le traitement défini dans le modèle
         ExécuteAncêtre
